        <!--==================== COMMENTS ====================-->
        <section class="comment section">
            <h2 class="section__title">Comments</h2>
            <span class="section__subtitle">Portfolio visitors comments</span>

            <div class="comment__container container grid">
                <i class="uil uil-comment-alt-notes comment__icon"></i>
                <ul class="comment__list">

                    <% if(comments.length > 0) { %>
                    <% comments.forEach((comment) => { %>
                    <li>
                        <div class="comment__item">
                            <h3 class="comment__item-name"><%= comment.name %></h3>
                            <p class="comment__item-message"><i
                                    class="uil uil-comment-alt-notes comment__icon-each"></i> <%= comment.message %></p>
                        </div>
                    </li>
                    <% }) %>
                    <% } else { %>
                    <p>No comments added yet</p>
                    <% } %>
                </ul>

                <form action="/comment" class="comment__form grid" method="post">
                    <div class="comment__inputs grid">
                        <div class="comment__content">
                            <label for="" class="comment__label">Name</label>
                            <input type="text" name="commentName" class="comment__input" required>
                        </div>
                    </div>
                    <div class="comment__content">
                        <label for="" class="comment__label">Comment</label>
                        <textarea name="commentMessage" id="" cols="0" rows="3" class="comment__input" required></textarea>
                    </div>

                    <div>
                        <button type="submit" class="btxdd button button--flex">
                            Add comment
                            <i class="uil uil-message button__icon"></i>
                        </button>
                    </div>
                </form>
            </div>


            
        </section>


        <!--==================== CONTACT ME ====================-->
        <section class="contact section" id="contactme">
            <h2 class="section__title">Contact Me</h2>
            <span class="section__subtitle">Get in touch</span>

            <div class="contact__container container grid">
                <div>
                    <div class="contact__information">
                        <i class="uil uil-phone contact__icon"></i>
                        <div>
                            <h3 class="contact__title">Call me</h3>
                            <span class="contact__subtitle">+61406585283</span>
                        </div>
                    </div>

                    <div class="contact__information">
                        <i class="uil uil-envelope contact__icon"></i>
                        <div>
                            <h3 class="contact__title">Email</h3>
                            <span class="contact__subtitle">mhaidarpoor@gmail.com</span>
                        </div>
                    </div>

                    <div class="contact__information">
                        <i class="uil uil-map-marker contact__icon"></i>
                        <div>
                            <h3 class="contact__title">Location</h3>
                            <span class="contact__subtitle">Brisbane, Australia</span>
                        </div>
                    </div>
                </div>

                <form action="/contact" class="contact__form grid" method="post">
                    <div class="contact__inputs grid">
                        <div class="contact__content">
                            <label for="name" class="contact__label">Name</label>
                            <input type="text" name="contact[name]" class="contact__input" required>
                        </div>
                        <div class="contact__content">
                            <label for="email" class="contact__label">Email</label>
                            <input type="email" name="contact[email]" class="contact__input" required>
                        </div>
                    </div>
                    <div class="contact__content">
                        <label for="project" class="contact__label">Project</label>
                        <input type="text" name="contact[project]" class="contact__input" required>
                    </div>
                    <div class="contact__content">
                        <label for="message" class="contact__label">Message</label>
                        <textarea name="contact[message]" id="" cols="0" rows="7" class="contact__input" required></textarea>
                    </div>

                    <div>
                        <button type="submit" class="button button--flex">
                            Send Message
                            <i class="uil uil-message button__icon"></i>
                        </button>
                    </div>
                </form>


            </div>

        </section>
